<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <title>HighTech - IT Solutions Website Template</title>
  <meta content="width=device-width, initial-scale=1.0" name="viewport">
  <meta content="" name="keywords">
  <meta content="" name="description">

  <!-- Google Web Fonts -->
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600&family=Saira:wght@500;600;700&display=swap" rel="stylesheet">

  <!-- Icon Font Stylesheet -->
  <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">

  <!-- Libraries Stylesheet -->
  <link href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css" rel="stylesheet">
  <link href="https://cdnjs.cloudflare.com/ajax/libs/owl-carousel/1.3.3/owl.carousel.min.js" rel="stylesheet">

  <!-- Customized Bootstrap Stylesheet -->
  <link href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap/5.3.2/css/bootstrap.min.css" rel="stylesheet">

  <!-- Template Stylesheet -->
  <link href="style.css" rel="stylesheet">
</head>

<body>
<!-- Spinner Start -->
<!-- Spinner End -->

<!-- Topbar Start -->
<div class="container-fluid bg-dark py-2 d-none d-md-flex">
  <div class="container">
    <div class="d-flex justify-content-between topbar">
      <div class="top-info">
        <small class="me-3 text-white-50"><a href="#"><i class="fas fa-map-marker-alt me-2 text-secondary"></i></a>08 Dinh Bat Tuy, Vinh, Vietnam</small>
        <small class="me-3 text-white-50"><%=link_to "Email@Example.com", contacts_path, class: 'fas fa-envelope me-2 text-secondary ' %> </small>
      </div>
      <div id="note" class="text-secondary d-none d-xl-flex"><small>Note : We help you to Grow your Business</small></div>
    </div>
  </div>
</div>
<!-- Topbar End -->

<div class="container-fluid bg-primary">
  <div class="container">
    <nav class="navbar navbar-dark navbar-expand-lg py-0">
      <%=image_tag"logosonant.jpg", style: "width:50px;height:50px"%> &ensp;
      <h1 class="text-white fw-bold d-block">Sonant</h1>
      <button type="button" class="navbar-toggler me-0" data-bs-toggle="collapse" data-bs-target="#navbarCollapse">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse bg-transparent" id="navbarCollapse">
        <div class="navbar-nav ms-auto mx-xl-auto p-0">
          <%=link_to "Home", root_path, class: 'nav-link' %>
          <%=link_to "About", about_path, class: 'nav-link' %>
          <%=link_to "Services", service_path, class: 'nav-link' %>
          <%=link_to "Jobs", jobs_path, class: 'nav-link' %>
          <%=link_to "Contact", contacts_path, class: 'nav-link' %>

        </div>
      </div>
      <div class="d-none d-xl-flex flex-shirink-0">
        <div id="phone-tada" class="d-flex align-items-center justify-content-center me-4">
          <a href="" class="position-relative animated tada infinite">
            <i class="fa fa-phone-alt text-white fa-2x"></i>
            <div class="position-absolute" style="top: -7px; left: 20px;">
              <span><i class="fa fa-comment-dots text-secondary"></i></span>
            </div>
          </a>
        </div>
        <div class="d-flex flex-column pe-4 border-end">
          <span class="text-white-50">Have any questions?</span>
          <span class="text-secondary text-white-50">Call: +16156678565</span>
        </div>
        <div class="d-flex align-items-center justify-content-center ms-4 ">
        </div>
      </div>
    </nav>
  </div>
</div>

<!-- Message Start -->
            <div class="content">
              <%= simple_form_for @contact, :html =>{ class: "container"} do |form| %>
                <br><br><br><br>
                <div class="row justify-content-center">
                  <div class="col-md-10">
                    <div class="row justify-content-center">
                      <div class=" form-group col-md-6">
                        <h3 class="heading mb-4">SEND US A MESSAGE</h3>

                        <%= image_tag "undraw-contact.svg", :class => "img-fluid",:alt => ""%>
                        <p>
                      </div>
                      <div class=" col-md-6">
                        <form class="mb-5" method="post" id="contactForm" name="contactForm" novalidate="novalidate"> <br><br>
                          <div class="row col-md-12 form-group">
                            <%form.label :name %>
                            <%=form.text_field :name, class: "form-control", placeholder: "Your name" %>
                            </div> <br>
                          <div class="row col-md-12 form-group">

                            <%form.label :email %>
                            <%=form.text_field :email, class: "form-control", placeholder: "Email address" %><br>
                          </div> <br>
                          <div class="row col-md-12 form-group">
                            <%form.label :message%>
                            <%=form.text_area :message, class: "form-control", cols: "30", rows: "7" ,placeholder: "Write your message" %>
                            </div> <br>
                          <div class="row">
                            <div class="col-12">
                              <input type="submit" value="Send Message" class="btn btn-primary rounded-0 py-2 px-4">
                              <span class="submitting"></span>
                            </div>
                          </div>
                        </form>
                      </div>
                    </div>
                  </div>
                </div>
                </div>
            <% end %>
              <!-- Footer Start -->

            <div class="container-fluid footer bg-dark wow fadeIn" data-wow-delay=".3s">
              <div class="container pt-5 pb-4">
                <div class="row g-5">
                  <div class="col-lg-3 col-md-6">
                    <<%=link_to "Home", root_path, class: 'nav-link' %>
                      <h1 class="text-white fw-bold d-block">Sonant</h1>
                    <p class="mt-4 text-light">Sonant Consulting Group works with international clients who have difficulties implementing VoIP. An array of customers all with unique telecom-related problems to be solved</p>
                  </div>
                  <div class="col-lg-3 col-md-6">

                  </div>
                  <div class="col-lg-3 col-md-6">
                    <a href="#" class="h3 text-secondary"></a>
                    <div class="mt-4 d-flex flex-column help-link">

                    </div>
                  </div>
                  <div class="col-lg-3 col-md-6">
                    <%=link_to" Contact Us", contacts_path, class: "h3 text-secondary"%>
                    <div class="text-white mt-4 d-flex flex-column contact-link">
                      <a href="#" class="pb-3 text-light border-bottom border-primary"><i class="fas fa-map-marker-alt text-secondary me-2"></i> 08 Dinh Bat Tuy, Vinh, Vietnam</a>
                      <a href="#" class="py-3 text-light border-bottom border-primary"><i class="fas fa-phone-alt text-secondary me-2"></i>  +16156678565</a>
                      <a href="#" class="py-3 text-light border-bottom border-primary"><i class="fas fa-envelope text-secondary me-2"></i> info@exmple.con</a>
                    </div>
                  </div>
                </div>
              </div>
    <!-- Footer End -->
    <!-- Back to Top -->
    <a href="#" class="btn btn-secondary btn-square rounded-circle back-to-top"><i class="fa fa-arrow-up text-white"></i></a>


    <!-- JavaScript Libraries -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="lhttps://cdnjs.cloudflare.com/ajax/libs/wow/1.1.2/wow.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.4.1/jquery.easing.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/waypoints/4.0.1/noframework.waypoints.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/owl.carousel.min.js"></script>

    <!-- Template Javascript -->
    <script src="javascript/application.js"></script>
  </div>
</body>

</html>


